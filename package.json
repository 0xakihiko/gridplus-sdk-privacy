{
  "name": "gridplus-sdk",
  "version": "0.0.7-dev2-unstable",
  "description": "SDK to interact with GridPlus agent devices",
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src test __tests__",
    "precommit": "npm run lint:fix && npm run test",
    "prepublish": "",
    "postinstall": "",
    "sleep": "sleep 20",
    "buildTokenList": "node support/getTokenList.js",
    "sendBTC": "node support/scripts/sendBTC.js",
    "sendETH": "node support/scripts/sendETH.js",
    "test": "mocha --timeout 120000 -R spec test/ --recursive --exit",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/GridPlus/gridplus-sdk.git"
  },
  "dependencies": {
    "aes-js": "^3.1.1",
    "bignumber.js": "^8.0.1",
    "bitcoinjs-lib": "^4.0.1",
    "bn.js": "^4.11.8",
    "buffer": "^5.1.0",
    "crc-32": "^1.2.0",
    "debug": "^4.1.0",
    "eccrypto": "^1.0.3",
    "elliptic": "^6.4.0",
    "ethers": "GridPlus/ethers.js#master",
    "gridplus-bclient": "^0.1.2",
    "left-pad": "^1.3.0",
    "semantic-release-cli": "^4.0.9",
    "sjcl": "^1.0.7",
    "superagent": "^3.8.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "commitizen": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^5.2.0",
    "jest": "^23.4.0",
    "meta-npm": "0.0.28",
    "mocha": "^5.2.0",
    "readline-sync": "^1.4.9",
    "semantic-release": "^15.11.0"
  },
  "files": [
    "index.js",
    "src",
    "support",
    "config.js"
  ],
  "license": "MIT"
}
